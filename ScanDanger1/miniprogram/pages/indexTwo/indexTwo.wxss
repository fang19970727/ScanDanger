page {
    width: 750rpx;
    height: 100%;
    font-family: -apple-system-font, Helvetica Neue, Helvetica, sans-serif;
}

.wrapper {
    padding: 5px;
}
.journey{
    font-size:60rpx;
    font-weight: bold;
    line-height:80rpx;
    color: rgb(12, 11, 10);
}
.journey-container{
    margin-top:100rpx;
    border: 1px solid rgba(234, 89, 60, 0);
    width: 200rpx;
    height:80rpx;
    border-radius:5px;
    text-align:center;
}
.flex-container {
    padding: 0;
    margin: 0 auto;
    list-style: none;
    display: flex;
    flex-flow: row;
    justify-content: space-around;
    line-height:1em;
    width: 90%;
}

.flex-item {
    background: #E6E9ED;
    margin: 1px;
    color: white;
    font-weight: bold;
    font-size: 10px;
    text-align: center;
    flex: 1 0 auto;
    height: 50rpx;
    width: 50rpx;
    font-family: "Lucida Console", Monaco, monospace
}
.open {
    background: #3BAFDA;
}
.mine {
    background: #E9573F;
}
.flag {
    
    background: #F6BB42;
}
.flex-item:before {
    content: '';
    display: block;
    float: left;
    padding-top:100%;
}
.header {
    display: flex;
    font-size: 14px;
    line-height: 2.5em;
    text-align: center;
}
.header .label {
    flex: 1;
}
.footer {
    padding-top: 50rpx;
    display: flex;   
    justify-content: space-around;
    align-items: center;
}

.button-hover {
    background-color: rgb(211, 211, 211);
}
.tips {
    margin-top: 20px;
    padding: 0 0 0 20px;
    font-size: 62.5%;
    color: #8d8d8d;
}
